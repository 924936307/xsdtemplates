<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:mytag="http://www.xsdtemplate/schema/mytag"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.xsdtemplate/schema/mytag
       http://www.xsdtemplate/schema/mytag.xsd
       ">
    <!--<mytag:user id="users" name="bobo" addr="oooo" age="18" gender="male"/>-->

    <bean class="com.xsd.template.schema.User">
        <property name="name" value="bobo"/>
